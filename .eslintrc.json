{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
     "react/jsx-filename-extension":[1,{"extensions": [".js", ".jsx"]}],
     "linebreak-style": 0,
     "react/react-in-jsx-scope": "off",
     "import/prefer-default-export": "off"
    }
}
